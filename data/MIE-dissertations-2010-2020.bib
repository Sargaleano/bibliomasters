@article{Ind.142,
author={Romero-Duque, Gu. and Olea-Suarez, Do},
title={Incidencias de las economias de aglomeracion en la productividad territorial : caso de estudio en la Localidad de Chapinero mediante analisis de datos espaciales: 2013-2017},
year={2020},
month={},
journal={OTROS},
note={cited By 0},
author_keywords={Analisis de datos espaciales; Economia de aglomeraciones; Eficiencia; Territorio},
keywords={datos; espaciales; Economia; aglomeraciones; Eficiencia; Territorio},
abstract={A traves del conocimiento de los comportamientos y las capacidades de un territorio (Llanos, 2018), se pueden determinar mejores formas para la gestion de politicas y estrategias que promuevan el desarrollo y bienestar de las ciudades (Mendoza & Capo, 2017). En los ultimos años, el desarrollo del territorio ha pasado a ser objeto de gran interes para sus stakeholders debido a la introduccion de aspectos espaciales en un ambito economico, lo que le permite a una ciudad, el reconocimiento de factores internos y externos (Bateman & Martinez, 2011) susceptibles a cambios en terminos de competitividad. La presente propuesta de investigacion se formula con el objetivo de proponer y desarrollar un modelo basado en Analisis de Datos Espaciales que permita medir el comportamiento de las dinamicas empresariales generadas en 19 sectores economicos del tejido productivo de la localidad de Chapinero de la ciudad de Bogota D.C. y explicar sus patrones de localizacion y concentracion geografica. La metodologia utilizada se orienta, en primer lugar, en caracterizar la tipologia empresarial existente en la localidad de chapinero mediante dos enfoques: (1) analizar las Unidades de Planeacion Zonal (en adelante UPZ) pertenecientes a localidad de Chapinero y definir las dinamicas empresariales que presenta cada UPZ a nivel de distribucion sectorial y desarrollo economico. (2) analizar la situacion financiera de los sectores economicos durante el periodo 2013-2017 presentes en la localidad, sus variaciones en terminos como capital de trabajo y nivel de endeudamiento, entre otros. En la siguiente etapa, se inicia el estudio de economias de aglomeracion basado en tecnicas de clustering particionado, combinando Analisis de Componentes Principales (PCA) y metodo de K-medias, las cuales son tecnicas de analisis multivariado para datas con multiples variables continuas y que permiten medir el comportamiento de las dinamicas empresariales generadas en diferentes sectores economicos del tejido productivo de la localidad de Chapinero. En esta etapa no interesa analizar aglomeraciones desde el aspecto geografico (vecindad), pero si interesa identificar y entender cuales sectores economicos han desarrollado estrategias afines (durante los años 2013-2017), enfocadas hacia apalancamientos desde los activos, pasivos, patrimonio y su relacion con las utilidades netas. El siguiente nivel de analisis es geografico, y busca desarrollar un analisis desde economias de localizacion o economias de urbanizacion (Duranton & Puga, 2004), permitiendo definir mediante la funcion k de Ripley si existen aglomeraciones de naturaleza industrial presentes en la localidad, entendida esta como la concentracion de la actividad (procesos similares) de un sector en particular (Rosenthal & Strange, 2004). Interesa saber si esta concentracion es dada por las relaciones y las logicas geograficas de organizacion productiva. El objeto es estimar si unidades empresariales instaladas en la localidad se estan beneficiando de las economias de aglomeracion, si existe diferencia entre sectores con o sin aglomeracion.},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_type={Thesis},
source={Repository},
}

@article{Ind.143,
author={Garzon-Luna, Ja. and Rueda-Velasco, Fe.},
title={Evaluacion de la localizacion de instalaciones y flujos de carga en la cadena de suministro de la Bienestarina Mas usando programacion matematica},
year={2020},
month={},
journal={OTROS},
note={cited By 0},
author_keywords={Problema de Localizacion y Asignacion;    Programas de Asistencia Alimentaria;  Alimentos de Alto Valor Nutricional; (Bienestarina Mas) ;Cadenas de Suministro; Humanitarias},
keywords={Localizacion; Asignacion; Programas; Asistencia; Alimentaria; Nutricional; Bienestarina; Cadenas; Suministro; Humanitarias},
abstract={La evaluacion de la configuracion estrategica de la cadena de suministro se considera una decision fundamental en logistica, especialmente en cadenas de suministro de asistencia alimentaria cuyo objetivo es mejorar las condiciones nutricionales de poblaciones en situaciones de vulnerabilidad. Colombia cuenta con un programa estatal denominado Bienestarina, que busca promover la seguridad alimentaria de los sectores mas vulnerables de la poblacion (primera infancia, niños, adolescente, embarazadas, mujeres lactantes, adultos mayores, personas enfermas y en situacion de discapacidad). Aunque el programa es apoyado por el gobierno y contribuye a mejorar las condiciones nutricionales, actualmente se presentan inconsistencias en los flujos de carga, falta de cobertura en algunas areas y demoras en las entregas. La tesis evalua la configuracion actual de la cadena y proponer mejoras relacionadas con la localizacion de instalaciones. Esta mejora permitiria el incremento en la eficacia de la red y la reduccion de la desnutricion en el pais. Para este proposito, se presenta un modelo de programacion matematica entera mixta que considera el criterio de distancia ponderada para diferentes escenarios de demanda. El modelo se codifico en el lenguaje de programacion algebraica GAMS. La configuracion de red actual se comparo con la estructura optima propuesta. Las comparaciones muestran grandes diferencias entre las asignaciones actuales y los escenarios propuestos por el modelo matematico, sugiriendo cambios en la localizacion actual de las bodegas regionales lo cual generaria implicaciones de politica publica para la reduccion del costo logistico en el sistema, priorizando a su vez la cobertura de la demanda.},
