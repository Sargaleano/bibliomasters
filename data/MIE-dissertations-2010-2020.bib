@article{Ind.136,
author={Mejia-Delgado, Cr. and Tarazona-Bermudez, Gi.},
title={Modelo para el suministro de uniformes del Ejercito Nacional de Colombia},
year={2020},
month={},
journal={GICOECOL},
note={cited By 0},
author_keywords={Cadena de Suministro; Optimizacion de Problemas Complejos; Industria 4.0; Gestion del conocimiento},
keywords={Cadena; Suministro; Optimizacion; Problemas; Complejos; Industria 4.0; Gestion del conocimiento},
abstract={Los desafios que presenta una economia global como la volatilidad en el precio de las materias primas (commodities), los servicios sociales de educacion y salud, el surgimiento cada vez mas frecuente de situaciones emergentes como fenomenos naturales o producto de las decisiones humanas, tienen como consecuencia la asignacion de recursos nacionales cada vez mas limitados para las cadenas de suministro del Ejercito Nacional de Colombia comparados con las necesidades, retos y requerimientos que presenta la fuerza en un ambito de confrontacion regular e irregular como al que se ven enfrentados los soldados de Colombia. Como respuesta este estudio pretende proponer un modelo de optimizacion para la cadena de suministro convencional del Ejercito Nacional especificamente la cadena de suministro de material de intendencia (uniforme No 1: camuflados), que involucre la implementacion de las nuevas tecnologias en informacion, comunicaciones y produccion o “Information, Communication, and Production Technologies” (ICPT), esta convergencia es llamada por la literatura academica como la cadena de suministro inteligente o “Smart Supply Chain Network” (SSCN). El modelo(s) se diseñara a partir de la caracterizacion de la cadena de suministro de camuflados fabricados en la planta de produccion del batallon las Juanas del Ejercito Nacional de Colombia, localizada en la ciudad de Bogota D.C. y cuya distribucion se dirige a suplir las necesidades a nivel nacional de este recurso fundamental para el ejercicio y cumplimiento constitucional. Las etapas de evaluacion y validacion de los modelos se implementaran a partir de experimentos computacionales y analisis de sensibilidad.},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_type={Thesis},
source={Repository},
}

