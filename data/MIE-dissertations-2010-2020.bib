@article{Ind.85,
author={Montana-Quintero, He. and Tarazona-Bermudez, Gi.},
title={Modelo Conceptual para el Desarrollo de Producto en el area de “Electronica” para la Universidad Distrital Francisco Jose de Caldas},
year={2017},
