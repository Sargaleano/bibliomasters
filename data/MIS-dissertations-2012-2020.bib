@article{Cic.1,
author={Vargas-Aguilar, Pa. and Salcedo-Parra, Oc.},
title={Modelo de Simulacion del protocolo de senalizacion RSVP-TE en redes IP/ MPLS para mejorar la calidad de servicio VoIP},
year={2012},
month={},
journal={Internet Inteligente},
note={cited By 0},
author_keywords={Redes conmutadas; Metodos de simulacion; Senalizacion; Sistemas de trasnmision de datos; Redes de computadores},
keywords={Redes; Conmutadas; Metodos; Simulacion; Senalizacion; Sistemas; Trasnmision; Datos; Computadores},
abstract={El rapido desarrollo de las tecnologias de redes de banda ancha ha supuesto significativos cambios en el mundo de las comunicaciones, el auge de los sistemas tecnologicos empleando soluciones MPLS (Multiprotocol Layer Switching Multiprotocolo de Switcheo de Etiquetas) y arquitecturas de servicios integrados que son empleados para garantizar calidad de servicio en los flujos de paquetes dentro de la red de un origen a un destino o de un origen a multiples destinos (Unicast, Multidifusion o Multicast) hace necesario el manejo de protocolos de comunicaciones que permitan ofrecer la calidad de servicio requerida para mejorar el desempeno de la red. Los protocolos de comunicaciones TCP/IP no aseguran esta calidad de servicio, es por esta razon que se presenta en esta Tesis un analisis de diferentes protocolos de comunicaciones apoyado en el desarrollado de un modelo de simulacion para visualizar el comportamiento de estos en la red. En este informe de investigacion se presenta un modelo de simulacion del protocolo de senalizacion RSVP-TE (Resource Reservation Protocol-Traffic Engineering) y del protocolo LDP (Label Distribution Protocol) en redes IP/MPLS (Internet Protocol / Multiprotocol Label Switching) para mejorar la calidad de servicio en VoIP, se define como la elaboracion, evaluacion y validacion de un modelo de simulacion del protocolo de senalizacion RSVP-TE comparandolo con el protocolo LDP en el nucleo de una red IP/MPLS para visualizar el comportamiento de los parametros de calidad de servicio en VoIP. El modelado de una solucion que a traves de la simulacion muestra como los parametros de calidad del servicio varian, permitiendo identificarlos, analizarlos, clasificarlos y modificarlos cambiando condiciones de la red IP/MPLS. El modelo de simulacion propuesto parte de la solucion del modelo analitico de balanceo de carga de flujos unicast aplicado a cada nodo de la red, adicionalmente se incluyen el diagrama de la red, las estadisticas y los parametros a medir, identificando los protocolos importantes que describen una red IP/MPLS tales como RSVP-TE y LDP al igual que los requerimientos de trafico teniendo en cuenta la tecnica de encolamiento WFQ (Weighted Fair Queuing). La construccion del modelo sigue las pautas de creacion, experimentacion y retroalimentacion, iterando hasta que el modelo muestre las mejores medidas de los parametros definidos para garantizar un buen servicio de VoIP tales como las perdidas de trafico, retardos, latencia, jitter y wander. Los resultados de la simulacion son analizados para asi seleccionar el modelo adecuado vinculado al protocolo RSVP-TE y al protocolo LDP, se realiza la implementacion del modelo, la evaluacion y validacion del mismo y se documentan las estadisticas obtenidas del comportamiento del protocolo de senalizacion demostrando el mejor control de los parametros de QoS (Quality of Service Calidad de Servicio) para la reserva de recursos en trafico de VoIP en la red IP/MPLS.},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

@article{Cic.2,
author={Silva-Suarez, Jo. and Angulo-Chavarro, Ja.},
title={Estudio de viabilidad para la implementacion de un modelo programa a nivel de especializacion en negociacion de tecnologias de la informacion y las comunicaciones},
year={2012},
month={},
journal={GITEM},
note={cited By 0},
author_keywords={Negociacion de tecnologias de la informacion y la comunicacion; Educacion superior; Negociacion de tecnologias de la informacion y la comunicacion; Planes de estudio; Educacion de postgrados},
keywords={Negociacion; Tecnologias; Informacion; Comunicacion; Educacion; Superior; Planes; Estudio; Postgrados},
abstract={La presente investigacion tuvo como finalidad realizar un estudio de viabilidad, para implementar un modelo programa a nivel de especializacion en negociacion de tecnologias de la informacion y las comunicaciones. El estudio se ubico en una investigacion de campo de caracter exploratorio descriptivo y mediante un analisis cualitativo y cuantitativo a partir de entrevistas e instrumentos tipo encuesta a ingenieros, gerentes, y empresarios, se analizo las necesidades de formacion del recurso humano de ingenieros de sistemas, electronicos, telematica y telecomunicaciones en las empresas del sector de las tics en Bogota en el area de gestion de los negocios. Se realiza muestreo no probabilistico a los programas de postgrado en las universidades para determinar si estos programas se adaptan a las necesidades que actualmente tienen las empresas en el area de gestion del negocio. Se pudo concluir que en Colombia no hay universidades que ofrezcan una Especializacion en Negociacion de Tecnologias de la Informacion y las Comunicaciones. Debido a lo anterior, se propone un modelo programa que contribuya a suplir las necesidades de formacion que actualmente tienen las empresas del sector en el area de gestion del negocio.},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

@article{Cic.3,
author={Rodriguez-Molano, Jo. and Perez-Castillo, Ne.},
title={Modelo de red social de emprendimiento para las pymes de la ciudad de Bogota},
year={2012},
month={},
journal={GICOGE},
note={cited By 0},
author_keywords={Microempresas; Emprendimiento; Nuevas empresas},
keywords={Microempresas; Emprendimiento; Nuevas empresas},
abstract={Este documento es un acercamiento al estado e importancia de las redes sociales de emprendimiento en la creacion, crecimiento y consolidacion de las PYMES y MIPYMES en Bogota, por cuanto ellas pueden consolidarse como un medio para que los emprendedores tengan mayor acceso a una gama de oportunidades, beneficios, asesorias y otros recursos pertinentes y exclusivos para su actividad emprendedora. Sin embargo, no basta con tener claras las necesidades de los emprendedores partiendo del analisis y diagnostico de las condiciones propias del negocio o de Bogota Region, tampoco es suficiente reconocer los actores clave que potencializan las PYMES y MIPYMES, tambien es necesario integrar el contenido y la estructura de la red social de emprendimiento mas adecuada para el escenario que presenta la ciudad capital a los emprendedores, enmarcandolo en un modelo. Lo anterior, por cuanto la utilidad, ventajas, requerimientos y propositos son diferentes segun sea el modelo de red social que se estudie, adicionalmente se debe tener en cuenta la etapa de la actividad de emprendimiento que se este desarrollando. Asi pues, este documento tiene su inicio cuando se presenta la situacion actual de PYMES y MIPYMES en Colombia, particularmente en la ciudad de Bogota y la tendencia de uso de redes sociales o similares en el proceso emprendedor. Posteriormente, se exponen definiciones necesarias para la comprension de las redes sociales de emprendimiento y los diferentes modelos y topologias de redes sociales, asi como su relacion con el emprendimiento. Tambien se esquematizan las principales redes de emprendimiento u oportunidades de acceso e integracion de emprendedores en Colombia tales como: Red de Emprendimiento Huila Emprende , Red de Emprendedores Bavaria , Bogota Emprende y Uniemprendedores .},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

@article{Cic.4,
author={Piedrahita-Villarraga, El. and Salcedo-Parra, Oc.},
title={Evaluacion de los protocolos de senalizacion del plano de control en redes GMPLS},
year={2012},
month={},
journal={Internet Inteligente},
note={cited By 0},
author_keywords={Sistemas de transmision de datos; Programas para computador; Protocolo de comunicaciones},
keywords={Sistemas; Transmision; Datos; Programas; Computador; Protocolo; Comunicaciones},
abstract={En esta tesis, se hace un estudio de la evolucion que han tenido las redes de comunicaciones hasta llegar a GMPLS: empezando por las redes de transporte como lo son SDH/SONET, WDM y redes opticas y continuando con MPLS. Luego, se realiza un contraste entre el protocolo RSVP-TE y CR-LDP basado en una revision detalla, identificacion de las principales diferencias e investigacion de nuevas propuestas para los protocolos de senalizacion. Para la parte del diseno del modelo de simulacion, se abarco la posibilidad de evaluar todas las variables que influyen en una comunicacion de extremo a extremo como los son las topologias de red con todos los enlaces y nodos que la conforman, los parametros fisicos de las fibras que incluyeron la posibilidad de simular el desempeno de cada enlace que usa en la topologia de red, y los parametros de QoS en el nodo que pueden incidir en un retardo debido al procesamiento. Posteriormente, se realizo una implementacion del modelo en el simulador GLASS, el cual permitio: caracterizar los enlaces propuestos en el diseno, realizar esquemas de fallas en la red, simular trafico sobre el LSP e implementar nodos electricos y opticos. La simulacion permite evaluar parametros como son Delay, Jitter y Throughput sobre las topologias definidas, para identificar fortalezas y debilidades de los protocolos. En la parte final se plantea una mejora que busca garantizar el buen desempeno del LSP en el protocolo de senalizacion RSVP-TE.},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

@article{Cic.5,
author={Pascuas-Rengifo, Yo. and Perez-Castillo, Ne.},
title={Modelo para el desarrollo de laboratorios virtuales de quimica bajo plataforma SAKAI empleando computacion GRID para la Universidad de la Amazonia  una aproximacion MDD para la generacion de SCORM},
year={2012},
month={},
journal={GICOGE},
note={cited By 0},
author_keywords={Sistemas de ensenanza; Redes de computadores; Tecnologia educativa; Software educativo},
keywords={Sistemas; ensenanza; Redes; computadores; Tecnologia; Software; educativo},
abstract={Los laboratorios virtuales apoyan y promueven el aprendizaje de los estudiantes y aumentan las opciones de experimentos disponbiles en las instituciones educativas. A pesar de las ventajas pedagogicas que tienen los laboratorios virtuales, se ha identificado que estas herramientas no cumplen con una estandarizacion que permita su interoperabilidda, reutilizacion y accesibilidad. Esto ocasiona, entre otras cosas, la creacion de aplicaciones no reutilizables y la inversion de tiempo y costo que esto supone. Como una posible solucion a los problemas mencionados, se propone la utilizacion del desarrollo dirigido por modelos para generar laboratorios virtuales que puedan ser empaquetados en el estandar SCROM y de esta forma, estar disponibles para uso en sistemas de gestion de aprendizaje},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}


@article{Cic.6,
author={Pardo-Pinzon, Ja. and Valero-Medina, Jo.},
title={Propuesta de interaccion entre sistemas heredados mediante notificaciones automaticas},
year={2012},
month={},
journal={NIDE},
note={cited By 0},
author_keywords={Sistemas de informacion; Telecomunicaciones; Redes de informacion},
keywords={Sistemas; Informacion; Telecomunicaciones; Redes; Informacion},
abstract={Existe una diversidad de organizaciones con unos requerimientos especificos del negocio, que trabajan aisladamente y generan un conjunto de bienes y servicios para toda una colectividad, pero, si bien es cierto que cada una de ellas tiene unas funciones especificas, es muy comun que para tomar algunas decisiones lo realicen con base en informacion generada por otras. Con la aparicion de las redes de computadores e internet, esta necesidad de compartir informacion encontro un escenario prometedor. Con el proposito de facilitar el acceso a la informacion, las diferentes organizaciones han realizado esfuerzos para poner en servicio, portales web que pueden ser accedidos tanto por funcionarios de otras entidades como por el publico en general que asi lo requiera. De esta manera, se ha logrado desarrollar un conjunto de herramientas eficaces para el acceso a informacion con la intervencion de un usuario final mediante interfaces flexibles y versatiles, sin embargo, la interaccion en linea y automatica entre entidades, asi como al interior de cada una de ellas ha resultado mas dificil, debido a que la mayoria de los sistemas locales en cada entidad no fueron implementados para este proposito, por lo que los mecanismos de interaccion que proporcionan son diversos o, aun en muchos de los casos nulos. Con el proposito de salvar este ultimo obstaculo, en las decadas pasadas se han desarrollado una serie de investigaciones que han dado como resultado tecnologias que proporcionan software de intermediacion, el cual facilita la implementacion de nuevos sistemas que basan su interaccion con otros en mecanismos y paradigmas tales como RPC1 (RemoteProcedureCall) o RMI (RemoteProcedureCall), los cuales deben ser considerados al implementar los nuevos sistemas},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

@article{Cic.7,
author={Osorio-Cabrera, Ch. and Medina-Daza, Ru.},
title={Fusion de imagenes satelitales mediante Wavelets para la maximizacion de la ganancia espectral},
year={2012},
month={},
journal={NIDE},
note={cited By 0},
author_keywords={Procesamiento de imagenes; Procesamiento optico de datos; Sensores remotos; Longitud de onda},
keywords={Procesamiento; imagenes; optico; datos; Sensores; remotos; Longitud; onda},
abstract={En este trabajo de investigacion se realizo un analisis de diferentes tecnicas de fusion de imagenes, pasando por los metodos tradicionales (Componentes Principales, Multiplicativo, Transformada de Brovey) hasta la fusion por medio del algoritmo de Mallat, usando tres familias de Wavelets (Haar, Meyer, Daubechies) para la ejecucion de los procesos de fusion. Se pretende considerar un conjunto de mecanismos adicionales que mejoran los procesos de fusion de imagenes para el estudio de la misma, y asi determinar por medio de una evaluacion cuantitativa basada en el analisis estadistico y matematico de los resultados (Correlacion, RASE, ERGAS, espectral y espacial) y de forma analoga una evaluacion cualitativa, analizando un conujnto de caracteristicas usuales (color, brillo, textura, forma) para asi concluir si dichos mecaismos adicionales aportaron en la obtencion de mejores fusiones},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

@article{Cic.8,
author={Ochoa-Rodriguez, Ju. and Vega-Torres, Ag.},
title={Condiciones de trabajo y salud ocupacional de la poblacion agricola del municipio de Chipaque Cundinamarca.},
year={2012},
month={},
journal={BIBLIOTECA VIRTUAL},
note={cited By 0},
author_keywords={Salud ocupacional; Prevencion de accidentes; Seguridad industrial; Produccion agropecuaria; Medidas de seguridad},
keywords={Salud; Ocupacional; Prevencion; Accidentes; Seguridad; Industrial; Produccion, Agropecuaria, Medidas},
abstract={Las actividades agricolas son tan antiguas como la humanidad, hace ocho mil anos se descubrio que las semillas recolectadas se convertian en plantas que crecian tan bien como las silvestres, lo que dio origen a la domesticacion de especies vegetales, a la que la siguio las especies animales, la superacion de la condicion de cazadores recolectores y su transformacion en campesinos. Recien hace tres mil anos la invencion del arado, permitio el surgimiento de la economia agricola. Por lo tanto el hecho de cultivar y sus formas han cambiado la realidad y el desarrollo de los pueblos, dando lugar a la necesidad de proteger y cualificar a los individuos en este tipo de actividades, valga resaltar las reiteradas advertencias de la OIT sobre el alto indice de mortalidad de los trabajadores agricolas, quienes corren al menos el doble de riesgos de morir en actividades relacionadas con su labor, que los trabajadores de otros sectores economicos. En Colombia historicamente la generacion de empleo en el campo han prevalecido las actividades de produccion de productos primarios, cultivos, ganaderia, extraccion maderera y reforestacion, para el caso de la agricultura se puede dividir en dos el latifundio y el minifundio, ademas se puede clasificar por tipo de cultivo, permanente o transitorio, dependiendo la region de pais que se estudie lo que por defecto incide en los niveles de ocupacion y empleabilidad tomando como base en la afiliacion al sistema general de seguridad social. En Colombia, segun la constitucion politica el Estado es el responsable de orientar, vigilar y controlar los riesgos profesionales elemento fundamental del sistema general de seguridad social integral, sin embargo al momento de conceptualizar y clasificar el trabajo de los individuos se puede ver desde dos orientaciones, el empleado y el ocupado, lo cual implica por derecho entrar a la formalidad e informalidad. Sin embargo las condiciones de trabajo y de salud en cualquier caso son determinantes para la sostenibilidad, productividad, competitividad de cualquier unidad productiva de la mano con las obligaciones del estado, la sociedad y las instituciones para garantizar la cobertura y los derechos irrenunciables de la persona y la comunidad para obtener calidad de vida acorde con la dignidad humana, reforzado la premisa que las condiciones de salud y seguridad son condiciones indispensables para el desarrollo socio economico de cualquier pais},
references={},
affiliation={Universidad Distrital Francisco Jose de Caldas, Bogota, Colombia},
language={Spanish},
document_Type={Thesis},
source={Repository},
}

